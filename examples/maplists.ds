MapOfLists:
    type R = {
        A: Int,
        B: String
    }
    type S = {
        B: String,
        C: Int
    }

    state Rs : Bag<R>
    state Ss : Bag<S>

    // TODO: fold this into q
    private query sumRB(b: String)
        sum [ s.C | s <- Ss, s.B == b ]

    query q()
        [(r.A, sumRB(r.B)) | r <- Rs ]

    op insert_r(r: R)
        Rs.add(r);

    op insert_s(s: S)
        Ss.add(s);